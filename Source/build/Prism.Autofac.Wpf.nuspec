<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Prism.Autofac</id>
    <version>$version$</version>
    <title>Autofac for Prism 7</title>
    <authors>Brian Lagunas, Bart Lannoeye, Dan Siegel</authors>
    <owners>Brian Lagunas, Bart Lannoeye, dansiegel</owners>
    <licenseUrl>https://github.com/PrismLibrary/Prism/blob/master/LICENSE</licenseUrl>
    <projectUrl>http://prismlibrary.com/</projectUrl>
    <iconUrl>http://prismlibrary.github.io/images/prism-logo-graphic-128.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Use these extensions to build Prism applications based on Autofac.</description>
    <summary>Autofac extensions for Prism.</summary>
    <releaseNotes>$releaseNotes$</releaseNotes>
    <language>en-US</language>
    <tags>prism mvvm wpf xaml autofac</tags>
    <dependencies>

      <group targetFramework=".NETFramework4.5">
        <dependency id="Autofac" version="4.6.2" />
        <dependency id="Prism.Wpf" version="$wpfVersion$" />
      </group>

    </dependencies>
  </metadata>
  <files>
    <file src="..\Wpf\Prism.Autofac.Wpf\bin\Release\Prism.Autofac.Wpf.*" target="lib\net45\" exclude="**\*.pdb" />
  </files>
</package>