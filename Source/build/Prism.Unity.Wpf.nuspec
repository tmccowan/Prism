<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Prism.Unity</id>
    <version>$version$</version>
    <title>Unity for Prism 7</title>
    <authors>Brian Lagunas, Bart Lannoeye, Dan Siegel</authors>
    <owners>Brian Lagunas, Bart Lannoeye, dansiegel</owners>
    <licenseUrl>https://github.com/PrismLibrary/Prism/blob/master/LICENSE</licenseUrl>
    <projectUrl>http://prismlibrary.com/</projectUrl>
    <iconUrl>http://prismlibrary.github.io/images/prism-logo-graphic-128.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Use these extensions to build Prism applications based on Unity.</description>
    <summary>Unity extensions for Prism.</summary>
    <releaseNotes>$releaseNotes$</releaseNotes>
    <language>en-US</language>
    <tags>prism mvvm wpf unity dependency injection di</tags>
    <dependencies>
      
      <group targetFramework=".NETFramework4.5">
        <dependency id="Unity.Container" version="5.8.3" />
        <dependency id="Prism.Wpf" version="$wpfVersion$" />
      </group>

    </dependencies>
  </metadata>
  <files>
    <file src="..\Wpf\Prism.Unity.Wpf\bin\Release\Prism.Unity.Wpf.*" target="lib\net45\" exclude="**\*.pdb;**\*.config" />    
  </files>
</package>